{
  "presets": [
    ["env", {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "Firefox ESR", "Safari >= 8"],
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    "react",
    "stage-2",
    ["es2015", {"modules": false}]
  ],
  "plugins": [
    "external-helpers",
    "syntax-dynamic-import",
    ["transform-class-properties", { "spec": true }]
  ]
}
